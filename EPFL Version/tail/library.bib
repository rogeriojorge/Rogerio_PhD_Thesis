journal = {Journal of Computational Physics},
number = {1},
pages = {243},
title = {{Gyrokinetic particle simulation model}},
url = {http://www.sciencedirect.com/science/article/pii/0021999187900805},
volume = {72},
year = {1987}
}
@article{Martin2008,
abstract = {The input power requirements for accessing H-mode at low density and$\backslash$nmaintaining it during the density ramp in ITER is addressed by statistical$\backslash$nmeans applied to the international H-mode threshold power database.$\backslash$nFollowing the recent addition of new data, the improvement of existing$\backslash$ndata and the improvement of selection criteria, a revised scaling$\backslash$nlaw that describes the threshold power required to obtain an L-mode$\backslash$nto H-mode transition is presented. Predictions for ITER give a threshold$\backslash$npower of {\~{}}52MW in a deuterium plasma at a line average density, n$\backslash$ne = 0.5×10 20 m -3 . At the nominal ITER H-mode density, n e = 1.0×10$\backslash$n20 m -3 , the threshold power required is {\~{}}86MW. Detailed analysis$\backslash$nof data from individual devices suggests that the density dependence$\backslash$nof the threshold power might increase with the plasma size and the$\backslash$nmagnetic field. On the other hand, the density at which the threshold$\backslash$npower is minimal is found to decrease with the plasma size and increase$\backslash$nwith magnetic field. The influence of these effects on the accessibility$\backslash$nof the H-mode regime in ITER plasmas is discussed. Analyses of the$\backslash$nconfinement database show that, in present day devices, H-modes are$\backslash$ngenerally maintained with powers exceeding the threshold power by$\backslash$na factor larger than 1.5, and that, on the other hand, good confinement$\backslash$ncan be obtained close to the threshold power although rarely demonstrated.},
