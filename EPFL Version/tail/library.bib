author = {Morimoto, Y. and Kondo, Y. and Kataoka, H. and Honda, Y. and Kozu, R. and Sakamoto, J. and Nakano, J. and Origuchi, T. and Yoshimura, T. and Okita, M.},
doi = {10.1007/s13398-014-0173-7.2},
