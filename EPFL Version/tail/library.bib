title = {{The Boltzmann Equation and the One-Fluid Hydromagnetic Equations in the Absence of Particle Collisions}},
url = {http://rspa.royalsocietypublishing.org/cgi/doi/10.1098/rspa.1956.0116},
volume = {236},
