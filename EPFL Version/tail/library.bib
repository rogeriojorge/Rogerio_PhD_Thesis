@article{Frieman1982,
abstract = {A nonlinear gyrokinetic formalism for low‐frequency (less than the$\backslash$ncyclotron frequency) microscopic electromagnetic perturbations in$\backslash$ngeneral magnetic field configurations is developed. The nonlinear$\backslash$nequations thus derived are valid in the strong‐turbulence regime$\backslash$nand contain effects due to finite Larmor radius, plasma inhomogeneities,$\backslash$nand magnetic field geometries. The specific case of axisymmetric$\backslash$ntokamaks is then considered and a model nonlinear equation is derived$\backslash$nfor electrostatic drift waves. Also, applying the formalism to the$\backslash$nshear Alfv{\'{e}}n wave heating scheme, it is found that nonlinear ion$\backslash$nLandau damping of kinetic shear‐Alfv{\'{e}}n waves is modified, both qualitatively$\backslash$nand quantitatively, by the diamagnetic drift effects. In particular,$\backslash$nwave energy is found to cascade in wavenumber instead of frequency.},
