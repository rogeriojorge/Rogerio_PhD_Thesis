title = {{Recent theoretical progress in understanding coherent structures in edge and SOL turbulence}},
volume = {74},
year = {2008}
}
@article{Chowdhury2010,
abstract = {An electron density gradient driven instability identified as the toroidal branch of the universal drift instability is studied using a global gyrokinetic model treating both electrons and ions fully nonadiabatically and valid at all orders in the ratio of the Larmor radius to the wavelength. The physics of the magnetic drift resonance, Landau resonance and transit resonance, which are considered to be important for the toroidal universal mode, are kept for both species. A systematic parametric study is carried out for the mode. The toroidal universal drift mode is observed to sustain finite temperature gradient and can thus coexist with the temperature gradient driven modes and may contribute to the observed particle transport along with other drift modes. Especially at intermediate scales between the ion temperature gradient driven mode and electron temperature gradient driven mode, this branch of the drift instability can also be a plausible candidate for the observed particle loss. The effect of magne...},
