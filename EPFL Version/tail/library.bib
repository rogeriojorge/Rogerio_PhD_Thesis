journal = {Physics of Fluids B: Plasma Physics},
keywords = {BALLOONING INSTABILITY,INSTABILITY GROWTH RATES,PLASMA CONFINEMENT,PLASMA DRIFT,SCALING LAWS,TEXT DEVICES,TOKAMAK DEVICES,TRANSPORT PROCESSES},
month = {mar},
number = {7},
pages = {1846},
publisher = {American Institute of Physics},
title = {{Stability of resistive and ideal ballooning modes in the Texas Experimental Tokamak and DIII-D}},
url = {http://aip.scitation.org/doi/10.1063/1.860038 http://scitation.aip.org/content/aip/journal/pofb/4/7/10.1063/1.860038},
volume = {4},
year = {1992}
}
