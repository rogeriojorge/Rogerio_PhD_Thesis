@article{Alves2007,
abstract = {This paper presents a tutorial on the fluid modelling of the positive column of direct-current cylindrical glow discharges. The model writes the continuity and momentum-transfer equations for electrons and ions, coupled with Poisson's equation and the electron mean energy transport equations. The model is solved for helium at pressures p{\{}sime{\}} 0.1 10 Torr, gas temperature T g = 500 K, discharge currents I dc = 5 200 mA, and tube radius R = 1 cm. The electron transport parameters and rate coefficients are calculated using the local mean energy approximation in articulation with a two-term Boltzmann solver. Model equations are integrated from the discharge axis to the wall, passing through the space-charge sheath and monitoring the charge separation within it. The results obtained allow a systematic characterization of discharge energy deposition and space-charge sheath formation, as a function of pressure and axial current. A general good agreement is found between model results and experimental measurements available in the literature.},
author = {Alves, Lu{\~{A}}­s L},
doi = {10.1088/0963-0252/16/3/015},
issn = {0963-0252},
journal = {Plasma Sources Sci. Technol.},
number = {3},
pages = {557},
title = {{Fluid modelling of the positive column of direct-current glow discharges}},
url = {http://stacks.iop.org/0963-0252/16/i=3/a=015},
volume = {16},
year = {2007}
}
@article{Naulin2007,
abstract = {In magnetically confined fusion experiments, turbulent transport dominates over collisional transport in the edge gradient region and in the scrape-off layer (SOL). Traditionally concepts as Fick's law are used to describe turbulent transport by effective diffusion coefficients and convective velocities. These concepts are only well founded if the transport exhibits Gaussian statistics. In the last decade it has become increasingly obvious this is not the case [E.T. Lu, Phys. Rev. Lett. 74 (1995) 2511; D. Newman, B.A. Carreras, P. Diamond, T.S. Hahm, Phys. Plasmas 3 (1996) 1858]. Intermittency, long range correlations, and ballistic transport events are widely documented in the plasma edge. The latter are characterized by the radial propagation of coherent structures - usually referred to as blobs - carrying energy, current and particle density across magnetic field lines. This ultimately renders the description of transport by mere use of effective mean transport coefficients useless, as this does not account for the effects of frequent extreme events, which have strong, lasting, and possibly destructive influence on plasma facing components. ?? 2007 Elsevier B.V. All rights reserved.},
author = {Naulin, V.},
doi = {10.1016/j.jnucmat.2006.12.058},
isbn = {0022-3115},
issn = {00223115},
journal = {Journal of Nuclear Materials},
keywords = {Cross-field transport,D0300,Edge modeling,Edge plasma,P0400,P0600,T0100},
number = {1},
pages = {24},
title = {{Turbulent transport and the plasma edge}},
volume = {363},
year = {2007}
}
@article{Cohen2008,
abstract = {Kinetic codes are required for quantitative simulation of edge plasmas$\backslash$nof most tokamaks, because orbit widths can be comparable to radial$\backslash$nscale lengths and because mean free paths can be comparable to scale$\backslash$nlengths along the magnetic field. However, the edge presents special$\backslash$nchallenges for edge simulation, both in terms of formulation and$\backslash$nimplementation. There are two major approaches to kinetic simulation,$\backslash$nnamely particle-based and continuum- (high-dimensional fluid-) based.$\backslash$nThe edge presents challenges common to both approaches as well as$\backslash$nones that are unique to each approach. In this paper we review these$\backslash$nchallenges, and survey how they are being addressed in current edge$\backslash$nkinetic simulation projects, as well as the status and accomplishments$\backslash$nof those projects. We discuss in some detail the status and recent$\backslash$naccomplishments of the {\{}U.S.{\}} Edge Simulation Laboratory {\{}(ESL),{\}}$\backslash$na project based on the continuum approach. The {\{}ESL{\}} currently consists$\backslash$nof a main-line effort to develop a code based on high-order conservative$\backslash$nfinite-volume discretization, as well as two prototype activities,$\backslash$n{\{}TEMPEST{\}} and {\{}EGK.{\}} These prototype codes are exploring issues attached$\backslash$nto energy-magnetic moment and parallel velocity-magnetic moment representations,$\backslash$nrespectively, as well as physics issues associated with simulation$\backslash$nin a steep radial gradient region and a domain that includes both$\backslash$nopen field lines and closed flux surfaces. (� 2008 {\{}WILEY-VCH{\}}$\backslash$nVerlag {\{}GmbH{\}} {\&} Co. {\{}KGaA,{\}} Weinheim)},
author = {Cohen, R. H. and Xu, X. Q.},
doi = {10.1002/ctpp.200810038},
issn = {08631042},
journal = {Contributions to Plasma Physics},
keywords = {Edge,Gyrokinetic,Kinetic,Plasma,Simulation},
number = {1-3},
pages = {212--223},
title = {{Progress in kinetic simulation of edge plasmas}},
volume = {48},
year = {2008}
}
@article{Hahm1988a,
abstract = {A nonlinear electrostatic gyrokinetic Vlasov equation as well as a Poisson equation have been derived in a form suitable for particle simulation studies of tokamak microturbulence and associated anomalous transport. This work differes from the existing nonlinear gyrokinetic theories in toroidal geometry, since the present equations conserve energy while retaining the crucial linear and nonlinear polarization physics. In the derivation, the action-variational Lie perturbation method is utilized in order to preserve the Hamiltonian structure of the original Vlasov-Poisson system. Emphasis is placed on the dominant physics of the collective fluctuations in toroidal geometry, rather than on the details of particle orbits.},
