title = {{Second order gyrokinetic theory for particle-in-cell codes}},
volume = {23},
year = {2016}
}
@article{Sanchez2000,
abstract = {The statistical properties of plasma fluctuations have been investigated in the plasma boundary region of fusion (tokamaks and stellarators) and nonfusion plasmas. Fluctuations in ion saturation current and floating potential have a near-Gaussian character in the proximity of the velocity shear layer (r(sh)). However, fluctuations deviate from a Gaussian distribution when moving inside of the plasma edge (r {\textless} r(sh)) or into the scrape-off layer region (r {\textgreater} r(sh)). Furthermore, fluctuations show sporadic pulses that are asymmetric in time. The present analysis shows a coupling of those pulses and the averaged flow in the shear layer region. (C) 2000 American Institute of Physics. [S1070-664X(00)00805-3]},
