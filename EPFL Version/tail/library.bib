@article{Zakharov1972,
abstract = {It is shown that arbitrary Langmuir turbulence of sufficient intensity (W /nT {\textgreater} (kr0 ) 2) is unstable. The instability leads to the development of significantly nonlinear phenomena, viz., the formation of low density regions (caverns) in the plasma which collapse during a finite time. Cavern collapse is an effective mechanism of energy dissipation of longwave Langmuir oscillations and plays an important role in the kinetics of Langmuir turbulency. The problem of the nonlinear stage of instability development of a monochromatic Langmuir wave is also considered.},
