@article{Sosenko2001,
abstract = {The asymptotic theory of charged particle motion in electromagnetic fields is developed for the general case of finite Larmor-radius effects by means of Krylov–Boholiubov averaging method. The correspondence between the general asymptotic methods, elaborated by M Krylov and M Boholiubov, the quasi-particle description and gyrokinetics is established. Such a comparison is used to shed more light on the physical sense of the reduced Poisson equation, introduced in gyrokinetics, and the particle polarization drift. The feasibility of numerical simulation schemes in which the polarization drift is included into the quasi-particle equations of motion, and the Poisson equation remains unchanged is analyzed.$\backslash$n$\backslash$nthe polarization drift},
