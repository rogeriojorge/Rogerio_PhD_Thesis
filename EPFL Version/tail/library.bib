isbn = {1070-664X},
issn = {1070664X},
journal = {Physics of Plasmas},
number = {12},
pages = {1},
title = {{Linearized model Fokker-Planck collision operators for gyrokinetic simulations. I. Theory}},
url = {http://arxiv.org/abs/0808.1300},
volume = {15},
year = {2008}
}
