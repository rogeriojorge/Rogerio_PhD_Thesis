@article{Vishniac2001,
abstract = {We construct a magnetic helicity conserving dynamo theory that incorporates a calculated magnetic helicity current. In this model the fluid helicity plays a small role in large-scale magnetic field generation. Instead, the dynamo process is dominated by a new quantity, derived from asymmetries in the second derivative of the velocity correlation function, closely related to the ``twist and fold'' dynamo model. The turbulent damping term is, as expected, almost unchanged. Numerical simulations with a spatially constant fluid helicity and vanishing resistivity are not expected to generate large-scale fields in equipartition with the turbulent energy density. The prospects for driving a fast dynamo under these circumstances are uncertain, but if it is possible, then the field must be largely force-free. On the other hand, there is an efficient analog to the $\alpha$-$\Omega$ dynamo. Systems whose turbulence is driven by some anisotropic local instability in a shearing flow, like real stars and accretion disks, and some computer simulations may successfully drive the generation of strong large-scale magnetic fields, provided that ∂r$\Omega${\textless}∂$\theta$vz$\omega$$\theta${\textgreater}{\textgreater}0. We show that this criterion is usually satisfied. Such dynamos will include a persistent, spatially coherent vertical magnetic helicity current with the same sign as -∂r$\Omega$, that is, positive for an accretion disk and negative for the Sun. We comment on the role of random magnetic helicity currents in storing turbulent energy in a disordered magnetic field, which will generate an equipartition, disordered field in a turbulent medium, and also a declining long-wavelength tail to the power spectrum. As a result, calculations of the Galactic ``seed'' field are largely irrelevant.},
archivePrefix = {arXiv},
arxivId = {astro-ph/0010373},
author = {Vishniac, E. and Cho, J.},
doi = {10.1086/319817},
eprint = {0010373},
issn = {{\textless}null{\textgreater}},
journal = {The Astrophysical Journal},
pages = {752},
primaryClass = {astro-ph},
title = {{Magnetic Helicity Conservation and Astrophysical Dynamos}},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data{\_}query?bibcode=2001ApJ...550..752V{\&}link{\_}type=ABSTRACT{\%}5Cnpapers2://publication/doi/10.1086/319817},
volume = {550},
year = {2001}
}
@article{Ricci2009,
abstract = {The methodology for plasma-turbulence code validation is discussed, with focus on the quantities to use for the simulation-experiment comparison, i.e., the validation observables, and application to the TORPEX basic plasma physics experiment [A. Fasoli , Phys. Plasmas 13, 055902 (2006)]. The considered validation observables are deduced from Langmuir probe measurements and are ordered into a primacy hierarchy, according to the number of model assumptions and to the combinations of measurements needed to form each of them. The lowest levels of the primacy hierarchy correspond to observables that require the lowest number of model assumptions and measurement combinations, such as the statistical and spectral properties of the ion saturation current time trace, while at the highest levels, quantities such as particle transport are considered. The comparison of the observables at the lowest levels in the hierarchy is more stringent than at the highest levels. Examples of the use of the proposed observables are applied to a specific TORPEX plasma configuration characterized by interchange-driven turbulence.},
author = {Ricci, P. and Theiler, C. and Fasoli, A. and Furno, I. and Labit, B. and Muller, S. and Podesta, M. and Poli, F.},
doi = {10.1063/1.3082698},
issn = {1070664X},
journal = {Physics of Plasmas},
number = {5},
pages = {055703},
title = {{Langmuir probe-based observables for plasma-turbulence code validation and application to the TORPEX basic plasma physics experiment}},
volume = {16},
year = {2009}
}
@article{Maggs2011,
abstract = {The dynamics of transport at the edge of magnetized plasmas is deterministic chaos. The connection is made by a previous survey [M. A. Pedrosa et al., Phys. Rev. Lett. 82, 3621 (1999)] of measurements of fluctuations that is shown to exhibit power spectra with exponential frequency dependence over a broad range, which is the signature of deterministic chaos. The exponential character arises from Lorentzian pulses. The results suggest that the generalization to complex times used in studies of deterministic chaos is a representation of Lorentzian pulses emerging from the chaotic dynamics},
author = {Maggs, J. and Morales, G.},
doi = {10.1103/PhysRevLett.107.185003},
issn = {00319007},
journal = {Physical Review Letters},
number = {18},
title = {{Generality of deterministic chaos, exponential spectra, and Lorentzian pulses in magnetically confined plasmas}},
volume = {107},
year = {2011}
}
