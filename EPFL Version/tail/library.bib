title = {{Fifty years in fusion and the way forward}},
volume = {50},
year = {2010}
}
@article{Xu2005,
abstract = {Intermittent convective transport has been investigated in the edge and the scrape-off layer (SOL) of TEXTOR using Langmuir probe signals. The probability distribution function (PDF) of the density fluctuations and the turbulence-induced flux are all positively skewed, while a Gaussian shape is recorded for the negative fluctuations. The deviation of the signals from Gaussian statistics clearly increases from the plasma edge to the SOL. Conditional averaging reveals that in the SOL region the waveform of intermittent structures is asymmetric in time and the burst events move radially outwards with Eth {\&}{\#}215; BT/B2 velocities of [?] 450 m s[?]1. It is found that the large burst fluctuations ([?] 2.5 {\&}{\#}215; rms) account for nearly 40{\%} of the total transport in the SOL. Statistics of the waiting-time between successive bursts indicate that the PDF of the time interval follows a Poisson-distribution for small-duration events (selected by size [?] 2.5 {\&}{\#}215; rms) and changes into a power-law form for larger ones. Moreover, the intermittency density fluctuation data clearly show self-similar characters and long-range time correlations through the presence of (1) sandpile-like frequency spectra and existence of the f[?]1 region; (2) a long tail in the autocorrelation function and (3) Hurst exponents H {\textgreater} 0.5 from R/S analysis, suggesting a possible role of avalanche-like transport in the turbulence intermittency},
author = {Xu, Y. and Jachmich, S. and Weynants, R. and Team, the TEXTOR},
doi = {10.1088/0741-3335/47/10/014},
isbn = {9781622763320},
issn = {0741-3335},
journal = {Plasma Physics and Controlled Fusion},
number = {10},
pages = {1841},
title = {{On the properties of turbulence intermittency in the boundary of the TEXTOR tokamak}},
url = {http://stacks.iop.org/0741-3335/47/i=10/a=014?key=crossref.ea816ecb6683ecf94805c5777cd3e49d},
volume = {47},
year = {2005}
}
@article{Bychenkov1994,
abstract = {A quasihydrodynamic description of ion acoustic waves (IAW) has been developed in the entire range of particle collisionality. It is shown that a frequency dependent 21‐moment Grad closure to ion fluid equations and phenomenological delocalized electron heat transport formulation can be used for an accurate analytical description of ion acoustic wave dispersion and damping. This description is in good agreement with the results of Fokker–Planck simulations. The authors' new model has been derived with the practical motivation of improving IAW description in large scale numerical modeling of ion wave instabilities. This theory has been also applied to the linear analysis of heat flux driven plasma instabilities. In particular, a new IAW instability in the regime of collisional electrons, which is induced by the density dependence of the electron thermal transport coefficient, has been found.},
