title = {{Measurements of interactions between waves and energetic ions in basic plasma experiments}},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data{\_}query?bibcode=2012PPCF...54l4007H{\&}link{\_}type=ABSTRACT{\%}5Cnpapers2://publication/doi/10.1088/0741-3335/54/12/124007},
volume = {54},
year = {2012}
}
