title = {{Convective transport by intermittent blob-filaments: Comparison of theory and experiment}},
volume = {18},
year = {2011}
}
@article{Tang1980,
abstract = {A systematic procedure for studying the influence of kinetic effects on the stability of MHD$\backslash$r ballooning modes is presented. The ballooning mode formalism, which is particularly effective for$\backslash$r analysing high-mode-number perturbations of a plasma in toroidal systems, is used to solve the$\backslash$r Vlasov-Maxwell equations for modes with perpendicular wavelengths on the scale of the ion$\backslash$r gyroradius. The local stability on each flux surface is determined by the solution of three coupled$\backslash$r integro-differential equations which include effects due to finite gyroradius, trapped particles,$\backslash$r and wave-particle resonances. More tractable forms of these equations are then obtained in the low$\backslash$r ($\omega$ {\textless} $\omega$ bi , $\omega$ ti ) and intermediate- ($\omega$ bi , $\omega$ ti {\textless} $\omega$ {\textless} $\omega$ be , $\omega$ te ) frequency regimes with $\omega$ bj$\backslash$r and $\omega$ tj being the average bounce and transit frequencies of each species. After further simplifying$\backslash$r approximations, the kinetic results here are shown to be reducible to the MHD-ballooning-mode$\backslash$r equations in the analogous limits, $\omega$ {\#}{\#}IMG{\#}{\#} [http://ej.iop.org/icons/Entities/lg.gif] {\{}lg{\}} $\omega$ s$\backslash$r where $\omega$ s = c s /L c , with c s being the acoustic speed and L c the connection length.},
