@article{Abramowicz1978,
abstract = {An analytic theory of the hydrodynamical structure of accreting disks (without self-gravitation but with pressure) orbiting around an axially symmetric, stationary, compact body (e.g., black hole) is presented. The inner edge of the marginally stable accreting disk (i.e., disk with constant angular momentum density) has a sharp cusp located on the equatorial plane no closer than the radius of the marginally bound circular orbit. The existence of the cusp is also typical for any angular momentum distribution. The physical importance of the cusp follows from the close analogy with the case of a close binary system (a Lagrange point on the Roche lobe). The existence of the cusp is thus a crucial phenomenon in such problems as boundary condition for the viscous stresses, accretion rate, etc.},
author = {Abramowicz, M. and Jaroszynski, M. and Sikora, M.},
journal = {Astronomy and Astrophysics},
keywords = {ANGULAR MOMENTUM,ASTRONOMICAL MODELS,BINARY STARS,BLACK HOLES (ASTRONOMY),CUSPS,GRAVITATIONAL EFFECTS,PRESSURE GRADIENTS,RELATIVITY,ROTATING DISKS,STELLAR MASS ACCRETION},
pages = {221--224},
title = {{Relativistic, accreting disks}},
url = {http://adsabs.harvard.edu/abs/1978A{\&}A....63..221A},
volume = {63},
year = {1978}
}
@article{Stegmeir2016,
abstract = {Predictions of plasma parameters in the edge and scrape-off layer of tokamaks is difficult since most modern tokamaks have a divertor and the associated separatrix causes the usually employed field/flux-aligned coordinates to become singular on the separatrix/X-point. The presented field line map approach avoids such problems as it is based on a cylindrical grid: standard finite-difference methods can be used for the discretisation of perpendicular (w.r.t. magnetic field) operators, and the characteristic flute mode property (k) of structures is exploited computationally via a field line following discretisation of parallel operators which leads to grid sparsification in the toroidal direction. This paper is devoted to the discretisation of the parallel diffusion operator (the approach taken is very similar to the flux-coordinate independent (FCI) approach which has already been adopted to a hyperbolic problem (Ottaviani, 2011; Hariri, 2013)). Based on the support operator method, schemes are derived which maintain the self-adjointness property of the parallel diffusion operator on the discrete level. These methods have very low numerical perpendicular diffusion compared to a naive discretisation which is a critical issue since magnetically confined plasmas exhibit a very strong anisotropy. Two different versions of the discrete parallel diffusion operator are derived: the first is based on interpolation where the order of interpolation and therefore the numerical diffusion is adjustable; the second is based on integration and is advantageous in cases where the field line map is strongly distorted. The schemes are implemented in the new code GRILLIX, and extensive benchmarks and numerous examples are presented which show the validity of the approach in general and GRILLIX in particular.},
archivePrefix = {arXiv},
arxivId = {arXiv:1505.02040v1},
author = {Stegmeir, A. and Coster, D. and Maj, O. and Hallatschek, K. and Lackner, K.},
doi = {10.1016/j.cpc.2015.09.016},
eprint = {arXiv:1505.02040v1},
issn = {00104655},
journal = {Computer Physics Communications},
keywords = {Field line map,Flux-coordinate independent approach,Numerical diffusion,Separatrix,Support operator,X-point},
pages = {139},
title = {{The field line map approach for simulations of magnetically confined plasmas}},
volume = {198},
year = {2016}
}
