issn = {0029-5515},
journal = {Nuclear Fusion},
number = {7},
pages = {073016},
title = {{Scrape-off layer properties of ITER-like limiter start-up plasmas in JET}},
url = {http://stacks.iop.org/0029-5515/53/i=7/a=073016?key=crossref.4107eea369c358b4d22e968611a90c6f},
volume = {53},
year = {2013}
