issn = {1742-6596},
journal = {Journal of Physics: Conference Series},
pages = {012033},
title = {{Power requirement for accessing the H-mode in ITER}},
url = {http://stacks.iop.org/1742-6596/123/i=1/a=012033?key=crossref.433acb98b7d8b044ce2b33de88f46da5},
volume = {123},
year = {2008}
