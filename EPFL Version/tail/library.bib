pages = {093031},
title = {{Scaling of the tokamak near the scrape-off layer H-mode power width and implications for ITER}},
url = {http://proxy-net.snu.ac.kr/6356f7/{\_}Lib{\_}Proxy{\_}Url/iopscience.iop.org/0029-5515/53/9/093031/article/},
volume = {53},
year = {2013}
