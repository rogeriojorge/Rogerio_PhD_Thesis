number = {2000},
pages = {1408},
title = {{Statistical characterization of fluctuation wave forms in the boundary region of fusion and nonfusion plasmas}},
url = {file:////media/DATOS/BIBLIOTECA/Journals/PHP01408.pdf{\%}5Cnfile:////media/DATOS/BIBLIOTECA/Sanchez2000{\#}1937{\_}CjnNVS.pdf{\%}5Cnhttp://link.aip.org/link/?PHPAEN/7/1408/1{\%}5Cnfile:////media/DATOS/BIBLIOTECA/Journals/PHP01408.pdf{\%}5Cnfile:////media/DATOS/BIBLIOTECA/Sanchez20},
volume = {7},
