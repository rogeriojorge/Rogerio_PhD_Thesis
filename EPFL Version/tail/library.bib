@article{Groebner1990,
abstract = {Marked changes in the edge radial electric field E r and in the edge poloidal rotation velocity v $\theta$ are important signatures of the L - H transition in the DIII-D tokamak. Shear exists in E r and v $\theta$ and increases from the L mode to the H mode. A comparison of experiment with theory shows that shear in E r and v $\theta$ is sufficient to suppress edge fluctuations, that ion-orbit loss is large enough to be the source of v $\theta$ , and that E r and v $\theta$ may play a causal role in the L - H transition.},
